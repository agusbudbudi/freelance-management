{
  "version": 2,
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node@3.0.7"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    }
  ],
  "buildCommand": "echo 'No build needed'",
  "outputDirectory": ".",
  "installCommand": "npm install --prefix api"
}
